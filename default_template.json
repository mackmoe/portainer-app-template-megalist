{
    "version": "2",
    "templates": [
      {
        "type": 1,
        "title": "Doozle",
        "description": "Live Container Logs, simple and elegant.",
        "administrator-only": false,
        "logo": "https://raw.githubusercontent.com/amir20/dozzle/master/assets/favicon.svg",
        "repository": {
            "url": "https://github.com/mackmoe/custom-compose-stacks",
            "stackfile": "management-tools/doozle/docker-compose.yml"
      },
        "platform": "linux",
        "categories": ["docker", "toolkit", "webUI"]
      },
      {
        "type": 1,
        "title": "Portainer",
        "description": "Web UI for manageing containerized applications with Docker.",
        "administrator-only": false,
        "image": "portainer/portainer-ee:latest",
        "name": "portainer",
        "logo": "https://raw.githubusercontent.com/portainer/portainer/develop/app/assets/ico/android-chrome-192x192.png",
        "volumes": [{
            "container": "/var/run/docker.sock",
            "bind": "/var/run/docker.sock"
        },
        {
            "container": "/data",
            "bind": "/data/compose/portainer"
        }],
        "ports": ["32772:9000"],
        "privileged": true,
        "restart_policy": "unless-stopped",
        "hostname": "portainer",
        "note": "This is the primary means to control docker so, run this template first.",
        "platform": "linux",
        "categories": ["docker", "toolkit", "webUI"]
      },
      {
        "type": 1,
        "title": "Ghost Blog",
        "description": "This is for my self-hosted ghost blog.",
        "administrator-only": false,
        "logo": "https://raw.githubusercontent.com/TryGhost/Ghost/main/ghost/admin/public/assets/img/logos/orb-pink-3.png",
        "repository": {
            "url": "https://github.com/mackmoe/custom-compose-stacks",
            "stackfile": "blogs/ghost/docker-compose.yml"
        },
        "platform": "linux",
        "categories": ["blog", "open-source"]
      },
      {
        "type": 1,
        "title": "Wordpress Blog",
        "description": "This is for my bae's wordpress blog we host.",
        "administrator-only": false,
        "logo": "https://avatars.githubusercontent.com/u/276006?s=200&v=4",
        "repository": {
            "url": "https://github.com/mackmoe/custom-compose-stacks",
            "stackfile": "blogs/wordpress/docker-compose.yml"
        },
        "platform": "linux",
        "categories": ["blog", "open-source"]
      }
    ]
  }