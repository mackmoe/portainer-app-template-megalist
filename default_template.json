{
    "version": "2",
    "templates": [
      {
        "type": 3,
        "title": "doozle",
        "description": "Live Container Logs, simple and elegant.",
        "image": "amir20/dozzle:latest",
        "administrator-only": false,
        "name": "doozle",
        "logo": "https://raw.githubusercontent.com/amir20/dozzle/master/assets/favicon.svg",
         "env": [{
            "name": "DOZZLE_ADDR",
            "label": "Application ip:port",
            "default": "0.0.0.0:8888",
            "preset": true,
            "description": "Needed for daemon/svc."
         }
        ],
        "volumes": [{
            "container": "/var/run/docker.sock",
            "bind": "/var/run/docker.sock"
        }],
        "ports": ["9999:8888"],
        "privileged": true,
        "restart_policy": "unless-stopped",
        "hostname": "doozle",
        "platform": "linux",
        "categories": ["docker", "toolkit"]
      }
    ]
  }